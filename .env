# Arquivo de variáveis de ambiente: /home/ubuntu/app_apostas/backend/.env

# Configurações do Servidor
PORT=3000

# Configurações do Banco de Dados PostgreSQL
DB_USER=postgres
DB_HOST=localhost
DB_NAME=app_apostas_db
DB_PASSWORD=postgres_password # Use uma senha forte em um ambiente real
DB_PORT=5432

# Segredo para JWT (JSON Web Token) - Use um valor forte e aleatório em produção
JWT_SECRET=esteEhUmSegredoMuitoForteParaJWT!@#$%
JWT_EXPIRES_IN=1h

